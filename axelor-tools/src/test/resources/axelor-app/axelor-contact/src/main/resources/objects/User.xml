<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_2.0.xsd">

  <module name="auth" package="com.axelor.auth.db" />

  <entity name="User">
    <string name="email" />
    <string name="testing" required="true"/>
    <finder-method name="findByEmail" using="email" />
    <extra-imports>
    import java.io.File;
    </extra-imports>
    <extra-code>
    public void doSomeThing() {
      System.out.println("blah blah");
    }
    </extra-code>
  </entity>

</domain-models>
