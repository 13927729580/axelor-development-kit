<?xml version="1.0" ?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.8.xsd">

  <module name="auth" package="com.axelor.auth.db"/>

  <entity name="Permission" sequential="true">
    <string name="name" unique="true" required="true"/>
    <string name="objectName" required="true" title="Object"/>
    <string name="condition"/>
    <string name="params"/>
    <boolean name="canRead" title="Read"/>
    <boolean name="canWrite" title="Write"/>
    <boolean name="canCreate" title="Create"/>
    <boolean name="canRemove" title="Remove"/>
  </entity>

</domain-models>
